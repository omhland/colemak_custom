partial alphanumeric_keys
xkb_symbols "kinesis" {

    include "us"
    name[Group1]= "English (Colemak)";

    key <TLDE> { [        aring,   Aring,      dead_tilde,       asciitilde ] };
    key <AE01> { [       exclam,       exclam,      exclamdown,      onesuperior ] };
    key <AE02> { [           at,           at,       masculine,      twosuperior ] };
    key <AE03> { [   numbersign,   numbersign,     ordfeminine,    threesuperior ] };
    key <AE04> { [       dollar,       dollar,            cent,         sterling ] };
    key <AE05> { [      percent,      percent,        EuroSign,              yen ] };
    key <AE06> { [  asciicircum,  asciicircum,         hstroke,          Hstroke ] };
    key <AE07> { [    ampersand,          eth,              ETH ] };
    key <AE08> { [     asterisk,     asterisk,           thorn,            THORN ] };
    key <AE09> { [    parenleft,    parenleft,  leftsinglequotemark,  leftdoublequotemark ] };
    key <AE10> { [  parentright,   parenright, rightsinglequotemark,  rightdoublequotemark ] };
    key <AE11> { [        minus,   underscore,          endash,           emdash ] };
    key <AE12> { [            z,            Z,        multiply,         division ] };

    key <AD01> { [            q,            Q,              q,               q ] };
    key <AD02> { [            w,            W,             equal,          w ] };
    key <AD03> { [            f,            F,           minus,          apostrophe ] };
    key <AD04> { [            p,            P,          plus,         quotedbl ] };
    key <AD05> { [            g,            G,     bar,       g ] };
    key <AD06> { [            j,            J,     question,          j ] };
    key <AD07> { [            l,            L,         comma,          semicolon ] };
    key <AD08> { [            u,            U,          period,           colon ] };
    key <AD09> { [            y,            Y,      exclam,       Udiaeresis ] };
    key <AD10> { [           grave,    grave,        grave,        grave, Odiaeresis ] };
    key <AD11> { [ 	     ae,           Ae,          Ae,        Ae ] };
    key <AD12> { [      	 a,  a,  a,        0x100203a ] };
    key <BKSL> { [    Aring,          quotedbl,      asciitilde,       asciitilde ] };

    key <AC01> { [            a,            A,               1,          at ] };
    key <AC02> { [            r,            R,               2,       numbersign ] };
    key <AC03> { [            s,            S,               3,        dollar ] };
    key <AC04> { [            t,            T,               4, 	percent ] };
    key <AC05> { [            d,            D,               5,       e ] };
    key <AC06> { [            h,            H,               6,       f ] };
    key <AC07> { [            n,            N,               7,           asciicircum ] };
    key <AC08> { [            e,            E,               8,           ampersand ] };
    key <AC09> { [            i,            I,               9,           asterisk ] };
    key <AC10> { [            o,            O,               0,           asciitilde ] };
    key <AC11> { [        Insert,       Insert,      Insert, Insert] };

    key <AB01> { [        z,            Z,     z,               Ae ] };
    key <AB02> { [            x,            X,      Delete,       asciitilde ] };
    key <AB03> { [            c,            C,      underscore,         Ae ] };
    key <AB04> { [            v,            V,      Insert,               OE ] };
    key <AB05> { [            b,            B,      b,       asciitilde ] };
    key <AB06> { [            k,            K,  k,       asciitilde ] };
    key <AB07> { [            m,            M,     Escape,       m ] };
    key <AB08> { [       parenleft,     bracketleft,    braceleft,       less ] };
    key <AB09> { [       parenright,      bracketright,   braceright,       greater ] };
    key <AB10> { [        slash,     backslash,    backslash,       asciitilde ] };

    key <CAPS> { [    BackSpace, Delete,      Insert,        BackSpace ] };
    key <LSGT> { [        oslash,  Oslash,            oslash,           Oslash ] };
    key <SPCE> { [        space,        Escape,              Escape,     escape ] };

    include "level3(ralt_switch)"
};

// I do NOT like dead-keys - the International keyboard as defined by Microsoft
// does not fit my needs. Why use two keystrokes for all simple characters (eg '
// and <space> generates a single ') just to have an é (eacute) in two strokes
// as well? I type ' more often than é (eacute).
//
// This file works just like a regular keyboard, BUT has all dead-keys
// accessible at level3 (through AltGr). An ë (ediaeresis) is now: AltGr+"
// followed by an e. In other words, this keyboard is not international as long
// as you leave the right Alt key alone.
