// Colemak symbols for xkb on X.Org Server 7.x
// 2006-01-01 Shai Coleman, http://colemak.com/

partial alphanumeric_keys
xkb_symbols "colemak_custom" {

    include "us"
    name[Group1]= "English (Colemak)";

    key <TLDE> { [        grave,   asciitilde,      dead_tilde,       asciitilde ] };
    key <AE01> { [       exclam,       exclam,      exclamdown,      onesuperior ] };
    key <AE02> { [           at,           at,       masculine,      twosuperior ] };
    key <AE03> { [   numbersign,   numbersign,     ordfeminine,    threesuperior ] };
    key <AE04> { [       dollar,       dollar,            cent,         sterling ] };
    key <AE05> { [      percent,      percent,        EuroSign,              yen ] };
    key <AE06> { [  asciicircum,  asciicircum,         hstroke,          Hstroke ] };
    key <AE07> { [    ampersand,          eth,              ETH ] };
    key <AE08> { [     asterisk,     asterisk,           thorn,            THORN ] };
    key <AE09> { [    parenleft,    parenleft,  leftsinglequotemark,  leftdoublequotemark ] };
    key <AE10> { [  parentright,   parenright, rightsinglequotemark,  rightdoublequotemark ] };
    key <AE11> { [        minus,   underscore,          endash,           emdash ] };
    key <AE12> { [            z,            Z,        multiply,         division ] };

    key <AD01> { [            q,            Q,              asterisk,               Aring ] };
    key <AD02> { [            w,            W,             minus,          endash ] };
    key <AD03> { [            f,            F,           equal,          emdash ] };
    key <AD04> { [            p,            P,          plus,         Ooblique ] };
    key <AD05> { [            g,            G,     asciicircum,       asciitilde ] };
    key <AD06> { [            j,            J,     ampersand,          Dstroke ] };
    key <AD07> { [            l,            L,         comma,          Lstroke ] };
    key <AD08> { [            u,            U,          period,           Uacute ] };
    key <AD09> { [            y,            Y,      exclam,       Udiaeresis ] };
    key <AD10> { [    semicolon,        dollar,      colon,       Odiaeresis ] };
    key <AD11> { [  percent,    numbersign,   at,        0x1002039 ] };
    key <AD12> { [ greater,   braceright,  guillemotright,        0x100203a ] };
    key <BKSL> { [    Ctrl,          quotedbl,      asciitilde,       asciitilde ] };

    key <AC01> { [            a,            A,               1,          at  ] };
    key <AC02> { [            r,            R,               2,       greater ] };
    key <AC03> { [            s,            S,               3,        3 ] };
    key <AC04> { [            t,            T,               4, 4 ] };
    key <AC05> { [            d,            D,               5,       asciitilde ] };
    key <AC06> { [            h,            H,               6,       asciitilde ] };
    key <AC07> { [            n,            N,               7,           parenleft ] };
    key <AC08> { [            e,            E,               8,           parenright ] };
    key <AC09> { [            i,            I,               9,           9 ] };
    key <AC10> { [            o,            O,               0,           0 ] };
    key <AC11> { [        apostrophe,    quotedbl,        underscore,           grave ] };

    key <AB01> { [        z,            Z,     bracketleft,               Ae ] };
    key <AB02> { [            x,            X,      bracketright,       asciitilde ] };
    key <AB03> { [            c,            C,      ae,         Ae ] };
    key <AB04> { [            v,            V,      underscore,               OE ] };
    key <AB05> { [            b,            B,      asterisk,       asciitilde ] };
    key <AB06> { [            k,            K,  asciitilde,       asciitilde ] };
    key <AB07> { [            m,            M,     question,       asciitilde ] };
    key <AB08> { [       parenleft,     less,    braceleft,       asciitilde ] };
    key <AB09> { [       parenright,      greater,   braceright,       asciitilde ] };
    key <AB10> { [        slash,     backslash,    bar,       asciitilde ] };

    key <CAPS> { [    BackSpace, Delete,      Insert,        BackSpace ] };
    key <LSGT> { [        aring,   Aring,            oslash,           Oslash ] };
    key <SPCE> { [        space,        Insert,              Escape,     escape ] };

    include "level3(ralt_switch)"
    include "ctrl(swap_ap_ctrl)"
};

// I do NOT like dead-keys - the International keyboard as defined by Microsoft
// does not fit my needs. Why use two keystrokes for all simple characters (eg '
// and <space> generates a single ') just to have an é (eacute) in two strokes
// as well? I type ' more often than é (eacute).
//
// This file works just like a regular keyboard, BUT has all dead-keys
// accessible at level3 (through AltGr). An ë (ediaeresis) is now: AltGr+"
// followed by an e. In other words, this keyboard is not international as long
// as you leave the right Alt key alone.
//
// The original MS International keyboard was intended for Latin1 (iso8859-1).
// With the introduction of iso8859-15, the (important) ligature oe (and OE)
// became available. I added them next to ae. Because I write ediaeresis more
// often than registered, I moved registered to be next to copyright and added
// ediaeresis and idiaeresis. - Adriaan

partial alphanumeric_keys
xkb_symbols "altgr-intl" {

   include "us(intl)"
   name[Group1]= "English (intl., with AltGr dead keys)";

// five dead keys moved into level3:

   key <TLDE> { [    grave, asciitilde,  dead_grave,   dead_tilde      ] };
   key <AC11> { [apostrophe,quotedbl,    dead_acute,   dead_diaeresis  ] };

// diversions from the MS Intl keyboard:

   key <AE01> { [        1, exclam,      onesuperior,  exclamdown      ] };
   key <AD04> { [        r, R,           ediaeresis,   Ediaeresis      ] };
   key <AC07> { [        j, J,           idiaeresis,   Idiaeresis      ] };
   key <AB02> { [        x, X,           oe,           OE              ] };
   key <AB04> { [        v, V,           registered,   registered      ] };

// onequarter etc (not in iso8859-15) moved to get three unshifted deadkeys:

   key <AE06> { [        6, asciicircum, dead_circumflex, onequarter    ] };
   key <AE07> { [        7, ampersand,   dead_horn,       onehalf       ] };
   key <AE08> { [        8, asterisk,    dead_ogonek,     threequarters ] };

   include "level3(ralt_switch)"
};

